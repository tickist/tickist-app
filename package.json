{
    "name": "tickist",
    "version": "1.0.0-beta.11",
    "license": "GPL-3.0-or-later",
    "repository": "https://github.com/tickist/tickist-app",
    "main": "dist/apps/tickist-functions/main.js",
    "engines": {
        "node": "18"
    },
    "scripts": {
        "ng": "nx",
        "nx": "nx",
        "start": "cross-env FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 nx serve --configuration=dev --host=0.0.0.0",
        "build": "nx build --configuration=production",
        "build:functions": "nx build \"tickist-functions\" --configuration=production",
        "test": "nx test",
        "lint": "nx run-many --all --target=lint",
        "cy:verify": "cypress verify",
        "e2e": "cross-env CYPRESS_FIREBASE_PROJECT_ID=proven-reality-657 FIREBASE_PROJECT_ID=proven-reality-657 FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 nx e2e tickist-web-e2e  --configuration ci --browser chrome",
        "e2e:watch": "cross-env CYPRESS_FIREBASE_PROJECT_ID=proven-reality-657 FIREBASE_PROJECT_ID=proven-reality-657 FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 nx --project tickist-web-e2e e2e --watch --browser chrome -c e2e --e2e",
        "e2e:watchWithEmulator": "firebase emulators:exec \"nx  e2e  tickist-web-e2e --watch -c ci --browser chrome --e2e \"  --import=./tools/e2e-testing-data",
        "e2e:watch:ci": "nx --project tickist-web-e2e e2e --watch -c ci",
        "e2e:testEmulator": "firebase emulators:start  --import=./tools/e2e-testing-data",
        "e2eCiWithEmulator": "firebase emulators:exec \"npm run e2e\" --import=./tools/e2e-testing-data",
        "startEmulator": "firebase emulators:start  --import=./tools/testing-data  --export-on-exit=./tools/testing-data",
        "test:emulate": "cross-env FIREBASE_DATABASE_EMULATOR_HOST=\"localhost:$(cat firebase.json | jq .emulators.database.port)\" FIRESTORE_EMULATOR_HOST=\"localhost:$(cat firebase.json | jq .emulators.firestore.port)\" yarn test:open",
        "deploy:development": "nx build && firebase deploy --project development",
        "deploy:staging": "nx build --configuration staging && firebase deploy --project staging",
        "deploy:production": "nx build --configuration production && firebase deploy --production --only functions",
        "firebase:serve": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase serve --only functions,firestore\"",
        "firebase:shell": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase functions:shell\" --raw",
        "deploy:development:functions": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase deploy --project development --only functions\"",
        "deploy:ci:functions": "concurrently --kill-others \"npm run build tickist-functions \" \"firebase deploy --project ci --only functions\"",
        "deploy:testing:functions": "concurrently --kill-others \"npm run build tickist-functions \" \"firebase deploy --project testing --only functions\"",
        "deploy:ci:rules": "firebase deploy --only firestore:rules --project ci --token $FIREBASE_TOKEN",
        "deploy:testing:rules": "firebase deploy --only firestore:rules --project testing",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "update": "nx migrate latest",
        "update:check": "FORCE_NG_UPDATE=true ng update",
        "workspace-schematic": "nx workspace-schematic",
        "dep-graph": "nx dep-graph",
        "help": "nx help"
    },
    "private": false,
    "optionalDependencies": {
        "fsevents": "1.2.9"
    },
    "dependencies": {
        "@angular/animations": "17.1.2",
        "@angular/cdk": "17.1.2",
        "@angular/common": "17.1.2",
        "@angular/core": "17.1.2",
        "@angular/fire": "16.0.0",
        "@angular/forms": "17.1.2",
        "@angular/material": "17.1.2",
        "@angular/platform-browser": "17.1.2",
        "@angular/platform-browser-dynamic": "17.1.2",
        "@angular/pwa": "17.1.2",
        "@angular/router": "17.1.2",
        "@angular/service-worker": "17.1.2",
        "@fortawesome/angular-fontawesome": "^0.14.0",
        "@fortawesome/fontawesome-free": "^6.4.2",
        "@fortawesome/fontawesome-svg-core": "^6.4.2",
        "@fortawesome/free-brands-svg-icons": "^6.4.2",
        "@fortawesome/free-regular-svg-icons": "^6.4.2",
        "@fortawesome/free-solid-svg-icons": "^6.4.2",
        "@ngbracket/ngx-layout": "^16.0.0",
        "@ngrx/effects": "17.0.1",
        "@ngrx/entity": "17.0.1",
        "@ngrx/eslint-plugin": "17.0.1",
        "@ngrx/router-store": "17.0.1",
        "@ngrx/store": "17.0.1",
        "@ngrx/store-devtools": "17.0.1",
        "@nx/angular": "18.0.1",
        "angular-resize-event": "^3.2.0",
        "chart.js": "^2.8.0",
        "core-js": "^3.19.1",
        "cross-env": "^7.0.3",
        "date-fns": "^2.0.1",
        "date-fns-timezone": "^0.1.4",
        "firebase": "^10.6.0",
        "firebase-admin": "^11.5.0",
        "firebase-functions": "^4.2.1",
        "firebase-tools": "^12.9.1",
        "fs-extra": "^8.1.0",
        "jq": "^1.7.2",
        "lodash": ">=4.17.21",
        "ngx-logger": "^5.0.11",
        "primeng": "17.0.0-beta.1",
        "quill": "^1.3.7",
        "ramda": "^0.27.0",
        "recursive-diff": "^1.0.0",
        "rxjs": "7.8.1",
        "truncate-html": "^1.0.3",
        "tsify": "^4.0.1",
        "tslib": "^2.0.0",
        "uuid": "^9.0.0",
        "zone.js": "0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/architect": "0.1701.2",
        "@angular-devkit/build-angular": "17.1.2",
        "@angular-devkit/core": "17.1.2",
        "@angular-devkit/schematics": "17.1.2",
        "@angular-eslint/eslint-plugin": "17.0.1",
        "@angular-eslint/eslint-plugin-template": "17.0.1",
        "@angular-eslint/template-parser": "17.0.1",
        "@angular-extensions/lint-rules": "^1.2.1",
        "@angular/cli": "~17.1.0",
        "@angular/compiler": "17.1.2",
        "@angular/compiler-cli": "17.1.2",
        "@angular/language-service": "17.1.2",
        "@cypress/webpack-preprocessor": "^5.16.1",
        "@ngrx/schematics": "17.0.1",
        "@nx/cypress": "18.0.1",
        "@nx/eslint-plugin": "18.0.1",
        "@nx/jest": "18.0.1",
        "@nx/node": "18.0.1",
        "@nx/workspace": "18.0.1",
        "@schematics/angular": "17.1.2",
        "@types/faker": "^4.1.4",
        "@types/jest": "29.4.0",
        "@types/node": "18.19.14",
        "@types/ramda": "^0.26.40",
        "@typescript-eslint/eslint-plugin": "6.20.0",
        "@typescript-eslint/parser": "6.20.0",
        "concurrently": "^4.1.2",
        "cypress": "^13.0.0",
        "cypress-firebase": "3.0.2",
        "dotenv": "10.0.0",
        "eslint": "8.48.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-cypress": "2.15.1",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-rxjs": "5.0.2",
        "faker": "^4.1.0",
        "firebase-functions-test": "^0.3.3",
        "firebase-tools": "^12.4.7",
        "fuzzy": "^0.1.3",
        "git-describe": "^4.0.4",
        "husky": "^1.3.1",
        "inquirer": "^6.2.2",
        "inquirer-autocomplete-prompt": "^1.0.1",
        "jest": "29.4.3",
        "jest-environment-jsdom": "29.4.3",
        "jest-preset-angular": "13.1.4",
        "ng-mocks": "14.12.0",
        "nx": "18.0.1",
        "prettier": "^3.1.0",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.1",
        "typescript": "5.3.3",
        "@nx/eslint": "18.0.1"
    }
}
