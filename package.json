{
    "name": "tickist",
    "version": "1.0.0-beta.9",
    "license": "GPL-3.0-or-later",
    "repository": "https://github.com/tickist/tickist-app",
    "main": "dist/apps/tickist-functions/main.js",
    "engines": {
        "node": "10"
    },
    "scripts": {
        "ng": "nx",
        "nx": "nx",
        "start": "nx serve -c dev --host 0.0.0.0",
        "build": "nx build --configuration production",
        "test": "nx test",
        "lint": "nx workspace-lint && nx run-many --fix --all --target=lint",
        "cy:verify": "cypress verify",
        "e2e": "cross-env CYPRESS_FIREBASE_PROJECT_ID=tickist-ci CYPRESS_TEST_UID=7mr64tVcVv3085oo0Y1VheOQYJXV FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 nx --project tickist-web-e2e e2e --configuration ci --record",
        "e2e:watch": "cross-env CYPRESS_FIREBASE_PROJECT_ID=proven-reality-657  CYPRESS_TEST_UID=7mr64tVcVv3085oo0Y1VheOQYJXV FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 nx --project tickist-web-e2e e2e --watch -c e2e",
        "e2e:watchWithEmulator": "firebase emulators:exec \"nx --project tickist-web-e2e e2e --watch -c e2e\"  --import=./tools/e2e-testing-data",
        "e2e:watch:ci": "nx --project tickist-web-e2e e2e --watch -c ci",
        "e2e:testEmulator": "firebase emulators:start  --import=./tools/e2e-testing-data",
        "e2eCiWithEmulator": "firebase emulators:exec \"npm run e2e\" --import=./tools/e2e-testing-data",
        "startEmulator": "firebase emulators:start  --import=./tools/testing-data  --export-on-exit=./tools/testing-data",
        "test:emulate": "cross-env FIREBASE_DATABASE_EMULATOR_HOST=\"localhost:$(cat firebase.json | jq .emulators.database.port)\" FIRESTORE_EMULATOR_HOST=\"localhost:$(cat firebase.json | jq .emulators.firestore.port)\" yarn test:open",
        "deploy:development": "nx build && firebase deploy --project development",
        "deploy:staging": "nx build --configuration staging && firebase deploy --project staging",
        "deploy:production": "nx build --configuration production && firebase deploy --production",
        "firebase:serve": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase serve --only functions,firestore\"",
        "firebase:shell": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase functions:shell\" --raw",
        "deploy:development:functions": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase deploy --project development --only functions\"",
        "deploy:ci:functions": "concurrently --kill-others \"npm run build tickist-functions \" \"firebase deploy --project ci --only functions\"",
        "deploy:testing:functions": "concurrently --kill-others \"npm run build tickist-functions \" \"firebase deploy --project testing --only functions\"",
        "deploy:ci:rules": "firebase deploy --only firestore:rules --project ci --token $FIREBASE_TOKEN",
        "deploy:testing:rules": "firebase deploy --only firestore:rules --project testing",
        "affected:apps": "nx affected:apps",
        "affected:libs": "nx affected:libs",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "update": "nx migrate latest",
        "update:check": "ng update",
        "workspace-schematic": "nx workspace-schematic",
        "dep-graph": "nx dep-graph",
        "help": "nx help",
        "firebase": "firebase"
    },
    "private": false,
    "optionalDependencies": {
        "fsevents": "1.2.9"
    },
