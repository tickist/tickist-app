{
  "name": "tickist",
  "version": "1.0.0-beta.6",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/tickist/tickist-app",
  "main": "dist/apps/tickist-functions/main.js",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve -c dev --host 0.0.0.0",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "cy:verify": "cypress verify",
    "e2e": "npm run build:ci:testConfig && ng e2e -c ci --headless --record --browser chrome",
    "e2e:watch": "npm run build:e2e:testConfig && ng e2e --watch -c e2e --browser chrome",
    "deploy:development": "ng build && firebase deploy --project development",
    "deploy:staging": "ng build --configuration staging && firebase deploy --project staging",
    "deploy:production": "ng build --prod && firebase deploy --production",
    "firebase:serve": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase serve --only functions,firestore\"",
    "firebase:shell": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase functions:shell\" --raw",
    "deploy:development:functions": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase deploy --project development --only functions\"",
    "deploy:ci:functions": "concurrently --kill-others \"npm run build tickist-functions \" \"firebase deploy --project ci --only functions\"",
    "deploy:ci:rules": "firebase deploy --only firestore:rules --project ci --token $FIREBASE_TOKEN",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "generate-git-version": "node grab-git-info.js",
    "build:testConfig": "cp ./cypress/config_ci.json ./cypress/config.json && cypress-firebase createTestEnvFile tickist-testing && mv cypress.env.json apps/tickist-web-e2e/",
    "build:ci:testConfig": "cp ./cypress/config_ci.json ./cypress/config.json && cypress-firebase createTestEnvFile tickist-ci && mv cypress.env.json apps/tickist-web-e2e/",
    "build:e2e:testConfig": "cp ./cypress/config_testing.json ./cypress/config.json && cypress-firebase createTestEnvFile && mv cypress.env.json apps/tickist-web-e2e/"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/fire": "^5.2.3",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.14",
    "@angular/material": "~8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/pwa": "^0.13.8",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@ngrx/effects": "^8.0.1",
    "@ngrx/entity": "^8.0.1",
    "@ngrx/router-store": "^8.0.1",
    "@ngrx/store": "^8.0.1",
    "@ngrx/store-devtools": "^8.0.1",
    "@nrwl/angular": "8.11.2",
    "chart.js": "^2.8.0",
    "core-js": "^2.5.4",
    "date-fns": "^2.0.1",
    "date-fns-timezone": "^0.1.4",
    "firebase": "^7.4.0",
    "firebase-admin": "^8.8.0",
    "firebase-functions": "^3.3.0",
    "hammerjs": "^2.0.8",
    "jq": "^1.7.2",
    "lodash": ">=4.17.11",
    "logrocket": "^0.6.20",
    "logrocket-ngrx": "^0.1.1",
    "ng2-charts": "^1.5.0",
    "ngx-sortablejs": "^3.1.3",
    "primeng": "^8.0.1",
    "quill": "^1.3.6",
    "rxjs": "^6.4.0",
    "sortablejs": "^1.9.0",
    "truncate-html": "^1.0.3",
    "tsify": "^4.0.1",
    "uuid": "^3.3.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "<0.900 || ^0.900.0-0 || ^9.0.0-0",
    "@angular-devkit/build-angular": "^0.803.22",
    "@angular/cli": "8.3.22",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@ngrx/schematics": "^8.0.1",
    "@nrwl/cypress": "8.11.0",
    "@nrwl/jest": "8.11.0",
    "@nrwl/node": "8.11.0",
    "@nrwl/workspace": "8.11.0",
    "@types/faker": "^4.1.4",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "concurrently": "^4.1.2",
    "cypress": "3.6.1",
    "cypress-firebase": "^0.4.1",
    "cypress-pipe": "^1.5.0",
    "dotenv": "6.2.0",
    "eslint": "6.1.0",
    "faker": "^4.1.0",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^6.10.0",
    "fuzzy": "^0.1.3",
    "git-describe": "^4.0.4",
    "husky": "^1.3.1",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "24.9.0",
    "jest-preset-angular": "7.0.0",
    "ng-mocks": "8.0.0",
    "ngrx-tslint-rules": "^1.14.1",
    "prettier": "1.19.1",
    "rxjs-tslint-rules": "^4.14.3",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3",
    "typescript-tslint-plugin": "^0.5.5"
  }
}
