{
  "name": "tickist",
  "version": "1.0.0-beta.11",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/tickist/tickist-app",
  "main": "dist/apps/tickist-functions/main.js",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "cross-env FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 nx serve -c dev --host 0.0.0.0",
    "build": "nx build --configuration production",
    "build:functions": "nx build \"tickist-functions\" --configuration production --with-deps",
    "test": "nx test",
    "lint": "nx workspace-lint && nx run-many --fix --all --target=lint",
    "cy:verify": "cypress verify",
    "e2e": "cross-env CYPRESS_FIREBASE_PROJECT_ID=proven-reality-657 FIREBASE_PROJECT_ID=proven-reality-657 FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 nx e2e tickist-web-e2e  --configuration ci --browser chrome",
    "e2e:watch": "cross-env CYPRESS_FIREBASE_PROJECT_ID=proven-reality-657 FIREBASE_PROJECT_ID=proven-reality-657 FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 nx --project tickist-web-e2e e2e --watch --browser chrome -c e2e --e2e",
    "e2e:watchWithEmulator": "firebase emulators:exec \"nx  e2e  tickist-web-e2e --watch -c ci --browser chrome --e2e \"  --import=./tools/e2e-testing-data",
    "e2e:watch:ci": "nx --project tickist-web-e2e e2e --watch -c ci",
    "e2e:testEmulator": "firebase emulators:start  --import=./tools/e2e-testing-data",
    "e2eCiWithEmulator": "firebase emulators:exec \"npm run e2e\" --import=./tools/e2e-testing-data",
    "startEmulator": "firebase emulators:start  --import=./tools/testing-data  --export-on-exit=./tools/testing-data",
    "test:emulate": "cross-env FIREBASE_DATABASE_EMULATOR_HOST=\"localhost:$(cat firebase.json | jq .emulators.database.port)\" FIRESTORE_EMULATOR_HOST=\"localhost:$(cat firebase.json | jq .emulators.firestore.port)\" yarn test:open",
    "deploy:development": "nx build && firebase deploy --project development",
    "deploy:staging": "nx build --configuration staging && firebase deploy --project staging",
    "deploy:production": "nx build --configuration production && firebase deploy --production --only functions",
    "firebase:serve": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase serve --only functions,firestore\"",
    "firebase:shell": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase functions:shell\" --raw",
    "deploy:development:functions": "concurrently --kill-others \"npm run build tickist-functions --watch\" \"firebase deploy --project development --only functions\"",
    "deploy:ci:functions": "concurrently --kill-others \"npm run build tickist-functions \" \"firebase deploy --project ci --only functions\"",
    "deploy:testing:functions": "concurrently --kill-others \"npm run build tickist-functions \" \"firebase deploy --project testing --only functions\"",
    "deploy:ci:rules": "firebase deploy --only firestore:rules --project ci --token $FIREBASE_TOKEN",
    "deploy:testing:rules": "firebase deploy --only firestore:rules --project testing",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "update:check": "FORCE_NG_UPDATE=true ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": false,
  "optionalDependencies": {
    "fsevents": "1.2.9"
  },
  "dependencies": {
    "@angular/animations": "15.0.3",
    "@angular/cdk": "15.0.2",
    "@angular/common": "15.0.3",
    "@angular/compiler": "15.0.3",
    "@angular/compiler-cli": "15.0.3",
    "@angular/core": "15.0.3",
    "@angular/fire": "^7.5.0",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "15.0.3",
    "@angular/material": "15.0.2",
    "@angular/platform-browser": "15.0.3",
    "@angular/platform-browser-dynamic": "15.0.3",
    "@angular/pwa": "^15.0.3",
    "@angular/router": "15.0.3",
    "@angular/service-worker": "15.0.3",
    "@fortawesome/angular-fontawesome": "^0.12.0",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@ngrx/effects": "15.0.0",
    "@ngrx/entity": "15.0.0",
    "@ngrx/router-store": "15.0.0",
    "@ngrx/store": "15.0.0",
    "@ngrx/store-devtools": "15.0.0",
    "@nrwl/angular": "15.4.4",
    "angular-resize-event": "^3.2.0",
    "chart.js": "^2.8.0",
    "core-js": "^3.19.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.0.1",
    "date-fns-timezone": "^0.1.4",
    "firebase": "^9.14.0",
    "firebase-admin": "^11.3.0",
    "firebase-functions": "^4.1.1",
    "fs-extra": "^8.1.0",
    "jq": "^1.7.2",
    "lodash": ">=4.17.21",
    "logrocket": "^3.0.1",
    "logrocket-ngrx": "^0.1.1",
    "ngx-logger": "^5.0.11",
    "ngx-sortablejs": "^11.1.0",
    "primeng": "^15.0.0-rc.1",
    "quill": "^1.3.7",
    "ramda": "^0.27.0",
    "recursive-diff": "^1.0.0",
    "rxjs": "7.6.0",
    "sortablejs": "^1.9.0",
    "truncate-html": "^1.0.3",
    "tsify": "^4.0.1",
    "tslib": "^2.0.0",
    "uuid": "^9.0.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1500.3",
    "@angular-devkit/build-angular": "15.0.3",
    "@angular-devkit/core": "15.0.3",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular-extensions/lint-rules": "^1.2.1",
    "@angular/cli": "~15.0.0",
    "@angular/language-service": "15.0.3",
    "@cypress/webpack-preprocessor": "^5.16.1",
    "@ngrx/schematics": "15.0.0",
    "@nrwl/cli": "15.4.4",
    "@nrwl/cypress": "15.4.4",
    "@nrwl/eslint-plugin-nx": "15.4.4",
    "@nrwl/jest": "15.4.4",
    "@nrwl/linter": "15.4.4",
    "@nrwl/node": "15.4.4",
    "@nrwl/workspace": "15.4.4",
    "@types/faker": "^4.1.4",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.1",
    "@types/ramda": "^0.26.40",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "concurrently": "^4.1.2",
    "cypress": "^11.2.0",
    "cypress-firebase": "3.0.1",
    "dotenv": "10.0.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-ngrx": "2.1.2",
    "eslint-plugin-rxjs": "5.0.2",
    "faker": "^4.1.0",
    "firebase-functions-test": "^0.3.3",
    "firebase-tools": "^11.16.1",
    "fuzzy": "^0.1.3",
    "git-describe": "^4.0.4",
    "husky": "^1.3.1",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "12.2.3",
    "ng-mocks": "12.5.0",
    "nx": "15.4.4",
    "prettier": "2.8.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
