{
    "npmScope": "tickist",
    "implicitDependencies": {
        "angular.json": "*",
        "package.json": "*",
        "tslint.json": "*",
        "nx.json": "*",
        "tsconfig.base.json": "*"
    },
    "projects": {
        "data": {
            "tags": []
        },
        "tickist-functions": {
            "tags": []
        },
        "tickist-web": {
            "tags": []
        },
        "tickist-web-e2e": {
            "tags": [],
            "implicitDependencies": ["tickist-web"]
        },
        "utils": {
            "tags": []
        }
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"]
            }
        }
    },
    "affected": {
        "defaultBase": "master"
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ]
    }
}
